//conditionals
//loops


public class Asts {
	
	public static void main(String[] args) {
			//conditionals can be expressions or true/false or boolean variables
			//booleans variables can be true/false or expressions
			int a = 5;
			boolean b = (a>10);
		
			if(b){
				System.out.println("TRUE");
			}else{
				System.out.println("FALSE");
			}
	}

}
