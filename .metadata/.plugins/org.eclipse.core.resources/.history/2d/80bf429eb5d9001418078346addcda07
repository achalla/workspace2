
public class QuickTest {
	
	public static void main(String[] args) {
		int j = 3/5;
		float f = 3/5;
		float g = (float)3/5;
		System.out.println(j);
		System.out.println(f);
		System.out.println(g);
		
		int arr[] = new int[6];
		for(int i = 0; i < arr.length; i++){
			System.out.println(arr[i]);
		}
	}

}
