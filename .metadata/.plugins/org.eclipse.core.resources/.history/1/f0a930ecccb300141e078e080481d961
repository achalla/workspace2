
public class Cat {
	
	String name;
	int age;
	boolean fat;
	
	public Cat(){
		name = "Fluffy";
		age = 10;		
	}
	
	public Cat(String inputName, int inputAge){
		name = inputName;
		age = inputAge;
	}
	
	
	
	public static void main(String[] args) {
		
		Cat c = new Cat("Snowball",5);
		System.out.println(c.age);
		System.out.println(c.name);
		System.out.println(c.fat);

	}

}
