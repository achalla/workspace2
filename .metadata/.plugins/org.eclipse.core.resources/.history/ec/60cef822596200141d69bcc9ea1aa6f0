package BinaryTree;

import java.util.ArrayList;

public class BST{

	static Node root;
	
	public static BST preToBST(int[] arr){
		int len = arr.length;
		root.datum = arr[0];
		Node temp = root, p;
		for(int i = 1; i < len; i++){
			Node n = new Node();
			n.datum = arr[i];
			
			if(arr[i] < arr[i-1]){
				temp.left = n;
			}else if(arr[i] > arr[i-1]){
				temp.right = n;
			}else{
				
			}
		}
		return null;
	}
	
